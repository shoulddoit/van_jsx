{
  "name": "@shoulddoit/van_jsx",
  "version": "1.0.3",
  "description": "van_jsx. A jsx helper for VanJS",
  "exports": {
    ".": "./dist/index.js"
  },
  "module": "dist/index.js",
  "type": "module",
  "types": "index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "index.d.ts",
    "h.js",
    "svg.js",
    "build.mjs",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "vanjs",
    "jsx",
    "UI",
    "UI Framework",
    "Reactive",
    "React"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.com/",
    "access": "public"
  },
  "scripts": {
    "build": "node build.js",
    "build-examples": "cd examples && node build.js"
  },
  "author": "zhanlefeng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shoulddoit/van_jsx.git"
  },
  "bugs": {
    "url": "https://github.com/shoulddoit/van_jsx/issues"
  },
  "homepage": "https://github.com/shoulddoit/van_jsx",
  "prettier": {
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "env": {
      "es2022": true,
      "commonjs": true,
      "browser": true,
      "node": true,
      "worker": true,
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ]
  },
  "devDependencies": {
    "esbuild": "^0.18.17",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.0.0",
    "vanjs-core": "^1.1.3"
  },
  "peerDependencies": {
    "vanjs-core": "^1.1.3"
  }
}
